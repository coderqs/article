&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> {
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> ip : process.env.MEDIASOUP_LISTEN_IP || &amp;#39;1.2.3.4&amp;#39;,
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> announcedIp : process.env.MEDIASOUP_ANNOUNCED_IP
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> },
&lt;/span>&lt;/span>&lt;span style="display:flex;">&lt;span> maxSctpMessageSize : 262144
